services:
  rabbitmq:
    container_name: rabbitmq
    environment:
      RABBITMQ_PASSWORD: password
      RABBITMQ_USERNAME: user
    image: rabbitmq:3-management
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    restart: always
  webapplication1:
    build:
      context: D:\massR\MassR
      dockerfile: WebApplication1/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: webapplication1
    ports:
    - published: 8085
      target: 80
    - published: 44003
      target: 443
version: '3.4'